{
  "Environment": {
    "prefix": "beg",
    "body": [
      "\\begin{$1}",
        "  $2",
      "\\end{$1}",    
      "$0"
    ]
  },

  "Inline math": {
    "prefix": "mm",
    "body": [
      "\\($1\\)$0"
    ]
  },

  "Align": {
    "prefix": "ali",
    "body": [
      "\\begin{align*}",
      "  $0",
      "\\end{align*}"
    ]
  },

  "Numbered align": {
    "prefix": "nali",
    "body": [
      "\\begin{align}",
      "  $0",
      "\\end{align}"
    ]
  },

  "Equation": {
    "prefix": "eq",
    "body": [
      "\\begin{equation*}",
      "  $0",
      "\\end{equation*}"
    ]
  },
	
  "Numbered equation": {
    "prefix": "neq",
    "body": [
      "\\begin{equation}",
      "  $0",
      "\\end{equation}"
    ]
  },

  "Enumerate": {
    "prefix": "enum",
    "body": [
      "\\begin{enumerate}[$1]",
      "\\  item $0",
      "\\end{enumerate}"
    ]
  },

  "Itemize": {
    "prefix": "item",
    "body": [
      "\\begin{itemize}",
      "\\  item $0",
      "\\end{itemize}"
    ]
  },

  "Left-right parenthesis": {
    "prefix": "lrp",
    "body": [
      "\\left( $1 \\right)$0"
    ]
  },

  "Left-right bracket": {
    "prefix": "lrb",
    "body": [
      "\\left[ $1 \\right]$0"
    ]
  },

  "Left-right braces": {
    "prefix": "lrbb",
    "body": [
      "\\left\\{ $1 \\right\\\\}$0"
    ]
  },

  "Left-right vertical": {
    "prefix": "lrv",
    "body": [
      "\\left| $1 \\right|$0"
    ]
  },

  "Left-right double vertical": {
    "prefix": "lrV",
    "body": [
      "\\left\\lVert $1 \\right\\lVert$0"
    ]
  },

  "Left-right angle": {
    "prefix": "lra",
    "body": [
      "\\left\\langle $1 \\right\\rangle$0"
    ]
  },

  "Left-right floor": {
    "prefix": "lrf",
    "body": [
      "\\left\\lfloor $1 \\right\\rfloor)$0"
    ]
  },

  "Left-right ceiling": {
    "prefix": "lrc",
    "body": [
      "\\left\\lceil $1 \\right\\rceil$0"
    ]
  },

  "Today": {
    "prefix": "today",
    "body": "$CURRENT_MONTH_NAME $CURRENT_DATE, $CURRENT_YEAR"
  },

  "Matrix": {
    "prefix": "mat",
    "body": [
      "\\begin{${1|matrix,pmatrix,bmatrix,Bmatrix,vmatrix,Vmatrix,smallmatrix,psmallmatrix,bsmallmatrix,Bsmallmatrix,vsmallmatrix,Vsmallmatrix|}}",
      "  $0",
      "\\end{${1|matrix,pmatrix,bmatrix,Bmatrix,vmatrix,Vmatrix,smallmatrix,psmallmatrix,bsmallmatrix,Bsmallmatrix,vsmallmatrix,Vsmallmatrix|}}"
    ]
  },

  "Standard template": {
    "prefix": "template",
    "body": [
      "\\documentclass{emilsart}",
      "\\title{$1}",
      "\\author{Emil Verkama}",
      "",
      "\\begin{document}",
      "\\maketitle",
      "$0",
      "\\end{document}"
    ]
  }
}
